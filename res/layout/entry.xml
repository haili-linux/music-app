<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:gravity="center_vertical"
	android:orientation="horizontal"
	android:id="@+id/entryMainLinearLayout"
	android:background="@drawable/image_30touming">

	<TextView
		android:layout_width="30dp"
		android:layout_height="30dp"
		android:text="69"
		android:gravity="center"
		android:id="@+id/entryTextView1"
		android:textColor="#000000"
		android:layout_marginLeft="7dp"/>

	<LinearLayout
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:gravity="left|center">

		<LinearLayout
			android:orientation="vertical"
			android:layout_marginLeft="5dp"
			android:layout_width="300dp"
			android:layout_height="wrap_content">

			<TextView
				android:layout_height="25dp"
				android:layout_width="wrap_content"
				android:textSize="16sp"
				android:text="测试226699"
				android:gravity="left|center"
				android:id="@+id/entryTextView_musiceName"
				android:textColor="#000000"/>

			<TextView
				android:layout_width="wrap_content"
				android:textSize="13sp"
				android:id="@+id/entryTextView_singer"
				android:layout_height="20dp"
				android:textColor="#212121"/>

		</LinearLayout>

		<TextView
			android:layout_height="40dp"
			android:layout_width="50dp"
			android:text="="
			android:background="@drawable/image_touming"
			android:gravity="center"
			android:textSize="20sp"
			android:textStyle="normal"
			android:id="@+id/entryTextView_setting"
			android:textColor="#000000"/>

	</LinearLayout>

</LinearLayout>

